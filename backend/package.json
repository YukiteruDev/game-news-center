{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc -b",
    "start": "tsx src/app.ts",
    "dev": "nodemon --watch src --ext ts --exec tsx src/app.ts",
    "dev:local": "dotenv -- nodemon --watch src --ext ts --exec tsx src/app.ts",
    "mikro-orm": "mikro-orm",
    "migrate:create": "tsx ./node_modules/@mikro-orm/cli/cli.js migration:create --config ./src/mikro-orm.config.ts",
    "migrate:up": "tsx ./node_modules/@mikro-orm/cli/cli.js migration:up --config ./src/mikro-orm.config.ts",
    "migrate:down": "tsx ./node_modules/@mikro-orm/cli/cli.js migration:down --config ./src/mikro-orm.config.ts",
    "migrate:create:local": "dotenv -- tsx ./node_modules/@mikro-orm/cli/cli.js migration:create --config ./src/mikro-orm.config.ts",
    "migrate:up:local": "dotenv -- tsx ./node_modules/@mikro-orm/cli/cli.js migration:up --config ./src/mikro-orm.config.ts",
    "migrate:down:local": "dotenv -- tsx ./node_modules/@mikro-orm/cli/cli.js migration:down --config ./src/mikro-orm.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.6.1",
  "dependencies": {
    "@mikro-orm/core": "^6.4.9",
    "@mikro-orm/migrations": "^6.4.9",
    "@mikro-orm/postgresql": "^6.4.9",
    "axios": "^1.8.2",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "cron": "^4.1.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "http": "0.0.1-security",
    "pg": "^8.13.3",
    "puppeteer": "^24.4.0",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.4.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.10",
    "@types/xml2js": "^0.4.14",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.11",
    "tsx": "^4.19.3",
    "typescript": "~5.7.3"
  },
  "optionalDependencies": {
    "@esbuild/linux-x64": "0.25.1"
  },
  "mikro-orm": {
    "useNodeTs": true,
    "configPaths": [
      "./src/mikro-orm.config.ts"
    ]
  }
}
